{
    "name": "@freddier's inception button",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "The inception button use by @freddier every epic streaming.",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "browser_action": {
        "default_icon": {         
          "19": "images/icon-19.png",      
          "38": "images/icon-38.png"          
        },
        "default_title": "@freddier's Inception Button",      
        "default_popup": "popup.html"       
    }
}